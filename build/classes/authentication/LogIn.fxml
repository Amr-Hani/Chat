   <children>
      <TextField fx:id="tf_username" layoutX="207.0" layoutY="83.0" promptText="Username" />
      <TextField fx:id="tf_password" layoutX="207.0" layoutY="157.0" promptText="Password" />
      <Button fx:id="btn_login" layoutX="207.0" layoutY="247.0" mnemonicParsing="false" onAction="#_login" text="LogIn" />
      <Button fx:id="btn_goToSignUp" layoutX="300.0" layoutY="247.0" mnemonicParsing="false" onAction="#_goToSignUp" text="GoToSinUp" />
